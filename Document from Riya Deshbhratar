   "source": [
    "## Data Cleaning \n",
    "\n",
    "df.columns = df.columns.str.strip()\n",
    "df['date_added'] = pd.to_datetime(df['date_added'],errors='coerce')\n",
    "df['year_added']=df['date_added'].dt.year\n",
    "df.dropna(subset=['type','title','rating','duration','date_added'],inplace=True)\n",
    "print('Cleaned Dataset Sample:')\n",
    "df.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "598377a1",
